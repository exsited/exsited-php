{
    "name": "mydemovendor/mydemopackage",
    "description": "A sample PHP project for Composer installation.",
    "type": "SDK",
    "license": "MIT",
    "require": {
        "psr/http-factory": "^1.1",
        "psr/http-client": "^1.0",
        "php-http/client-common": "^2.7"
    },
    "require-dev": {
        "php-http/curl-client": "^2.3",
        "laminas/laminas-diactoros": "2.17",
        "psr/http-message": "1.0"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "autoload": {
        "psr-4": {
            "Api\\": "src/"
        }
    }
}
